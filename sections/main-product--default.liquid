{%- liquid
  assign current_variant = product.selected_or_first_available_variant
  assign featured_media = current_variant.featured_media | default: product.featured_media
  assign first_3d_model = product.media | where: 'media_type', 'model' | first
  assign gallery_style = section.settings.gallery_style
  assign gallery_size = section.settings.gallery_size
  assign gallery_overflow = section.settings.gallery_overflow
  assign image_aspect_ratio = section.settings.image_aspect_ratio

  if gallery_style == 'thumbnails'
    assign gallery_overflow = 'never'
  endif

  # If only single image in gallery display as list
  if product.media.size == 1
    assign gallery_style = 'list'
  endif

  if gallery_size == 'small'
    assign desktop_image_container_percentage = 46
  elsif gallery_size == 'medium'
    assign desktop_image_container_percentage = 56
  elsif gallery_size == 'large'
    assign desktop_image_container_percentage = 64
  elsif gallery_size == 'xlarge'
    assign desktop_image_container_percentage = 72
  endif

  if gallery_style == 'grid'
    assign desktop_image_percentage_width = desktop_image_container_percentage | divided_by: 2
  else
    assign desktop_image_percentage_width = desktop_image_container_percentage
  endif

  assign media_show_limit = product.media.size
  assign over_media_show_limit = false

  unless gallery_overflow == 'never'
    assign media_per_row = 1
    if gallery_style == 'grid'
      assign media_per_row = 2
    endif

    assign gallery_overflow_number = gallery_overflow | times: 1.0
    assign media_limit = gallery_overflow_number | times: media_per_row
    # add 1 because n+1th row should be partially visible
    assign media_show_limit = gallery_overflow_number | plus: 1 | times: media_per_row
    assign transition_multiplier = product.media.size | times: 1.0 | divided_by: media_per_row | ceil | minus: gallery_overflow_number | times: media_per_row | at_most: 4
    assign overflow_gallery_aspect_ratio = 'auto'

    if media_limit < product.media.size
      assign over_media_show_limit = true

      if image_aspect_ratio == 'natural'
        assign cumulative_height = 0

        for media in product.media
          if forloop.index > media_show_limit
            break
          endif

          unless media_per_row == 1
            assign remainder = forloop.index | modulo: media_per_row

            if remainder == 0
              continue
            endif

            assign next_inverse_aspect_ratio = 0
            assign next_media = product.media[forloop.index]

            unless next_media == blank
              assign next_inverse_aspect_ratio = 1
              unless next_media.type == 'model'
                assign next_inverse_aspect_ratio = 1 | divided_by: next_media.aspect_ratio
              endunless
            endunless
          endunless

          assign inverse_aspect_ratio = 1
          unless media.type == 'model'
            assign inverse_aspect_ratio = 1 | divided_by: media.aspect_ratio
          endunless

          assign tallest_in_row = inverse_aspect_ratio | at_least: next_inverse_aspect_ratio

          if forloop.index > media_limit
            # Calculate tease row
            assign tallest_in_row = 0.35 | times: media_per_row
          endif

          assign cumulative_height = cumulative_height | plus: tallest_in_row
        endfor

        assign overflow_gallery_aspect_ratio = 1 | divided_by: cumulative_height | times: media_per_row
      else
        if image_aspect_ratio == 'portrait'
          assign image_fill_aspect_ratio = 0.66666
        elsif image_aspect_ratio == 'landscape'
          assign image_fill_aspect_ratio = 1.5
        elsif image_aspect_ratio == 'square'
          assign image_fill_aspect_ratio = 1
        endif

        # Multiply by aspect ratio to maintain a constant height for the tease row,
        # instead of a constant percentage visible (which will vary with image height)
        assign tease_row_ratio = 0.35 | times: image_fill_aspect_ratio | times: media_per_row

        assign height = gallery_overflow_number | plus: tease_row_ratio
        assign width = image_fill_aspect_ratio | times: media_per_row

        assign overflow_gallery_aspect_ratio = width | divided_by: height
      endif
    endif
  endunless
-%}

{% comment %}account for gutter between columns and page outer space{% endcomment %}
{%- capture desktop_image_sizes_attribute -%}
  {{ desktop_image_percentage_width }}vw{%- endcapture -%}

{%- capture product_url -%}
  {{ shop.url | append: product.url }}{%- endcapture -%}
{%- assign featured_media_size = featured_media.width | append: 'x' -%}
{%- if product.featured_media -%}
  {%- capture product_featured_image_url -%}
    {{- product.featured_media | image_url: featured_media_size | prepend: 'https:' | json -}}
  {%- endcapture -%}
{% endif %}
<div class="
      product
      {% if settings.enable_section_animations and section.settings.enable_animation %}
        animation
        animation--product
      {% endif %}
    " data-section-id="{{ section.id }}" data-section-type="product" data-current-product-id="{{ current_variant.id }}" data-product-has-only-default-variant="{{ product.has_only_default_variant }}" data-enable-sticky-product-details="{{ section.settings.enable_sticky_product_details }}" data-gallery-size="{{ gallery_size }}" data-is-full-product="true">
  {% if section.settings.show_breadcrumbs or section.settings.show_navigation_buttons %}
    <div class="product__breadcrumbs">
      {% render 'breadcrumbs'
       , show_breadcrumbs: section.settings.show_breadcrumbs
       , show_navigation_buttons: section.settings.show_navigation_buttons
       %}
    </div>
  {%- endif -%}
  <div class="product__top product__primary">
    <div class="product__primary-left">
      <div class="product__media-container below-mobile carousel swiper{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}">
        <div class="product__media carousel__wrapper swiper-wrapper">
          {%- for media in product.media -%}
            {% assign prioritize_loading = false %}
            {% if forloop.first %}
              {% assign prioritize_loading = true %}
            {% endif %}
            <div class="product__media-item carousel_slide swiper-slide" data-media-item-id="{{ media.id }}" data-media-type="{{ media.media_type }}" data-product-media-wrapper data-aspect-ratio="{{ image_aspect_ratio }}">
              {% render 'media'
               , fallback_alt: product.title
               , media: media
               , featured_media: featured_media
               , enable_product_lightbox: section.settings.enable_product_lightbox
               , loop_video: section.settings.enable_video_looping
               , aspect_ratio: image_aspect_ratio
               , sizes: '90vw'
               , prioritize_loading: prioritize_loading
               , within_carousel: true
               %}
            </div>
          {%- else -%}
            <div class="product__media-item placeholder-image">
              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
          {%- endfor -%}
        </div>
        <div class="swiper-pagination"></div>
      </div>
      <div class="product__media-container above-mobile{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}" data-gallery-style="{{ gallery_style }}">
        {% if gallery_style == 'thumbnails' %}
          {% render 'product-thumbnails', featured_media: featured_media, aspect_ratio: image_aspect_ratio %}
        {% endif %}
        <div class="product__media" {% if over_media_show_limit %}data-product-media="closed"style="
                        --overflow-gallery-aspect-ratio: {{ overflow_gallery_aspect_ratio }};
                        --overflow-gallery-transition-multiplier: {{ transition_multiplier }};
                      "{%- endif -%}>{%- for media in product.media -%}
            <div class="
                              product__media-item
                              {% if forloop.index0 >= media_show_limit %}product__media-item--hide-on-load{% endif %}
                              {% unless media.id == featured_media.id or gallery_style != "thumbnails" %} hidden{% endunless %}
                              animation--product-media
                            " data-media-type="{{ media.media_type }}" data-media-item-id="{{ media.id }}" data-media-type="{{ media.media_type }}" data-product-media-wrapper {% if gallery_style == 'grid' or media.media_type == 'image' or media.media_type == 'model' %}data-aspect-ratio="{{ image_aspect_ratio }}"{% endif %}>{% render 'media'
 , media: media
 , fallback_alt: product.title
 , featured_media: featured_media
 , enable_product_lightbox: section.settings.enable_product_lightbox
 , loop_video: section.settings.enable_video_looping
 , aspect_ratio: image_aspect_ratio
 , sizes: desktop_image_sizes_attribute
 , animate: true
 %}
            </div>
          {%- else -%}
            <div class="product__media-item placeholder-image">
              {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
          {%- endfor -%}
          {%- if over_media_show_limit -%}
            <div class="product__media-show-more" data-more-media-bar>
              <button class="btn btn--tertiary btn--small" type="button" data-more-media data-lang-more-media="{{ 'products.product.more_media' | t | escape }}" data-lang-less-media="{{ 'products.product.less_media' | t | escape }}">
                {{ 'products.product.more_media' | t }}
              </button>
            </div>
          {%- endif -%}
        </div>
      </div>

      {%- if first_3d_model -%}
        <button aria-label="{{ 'products.product.view_in_space_label' | t }}" class="product__view-in-space btn btn--primary btn--full" data-in-your-space data-shopify-xr data-shopify-model3d-id="{{ first_3d_model.id }}" data-shopify-title="{{ product.title | escape }}" data-shopify-xr-hidden>
          <span class="fs-body-75">
            {% render 'icon', icon: '3d' %}
            <span class="product__view-in-space-text">{{ 'products.product.view_in_space' | t }}</span>
          </span>
        </button>
      {%- endif -%}

      {%- capture left_side_blocks -%}
        {%- render 'product-blocks', blocks: section.blocks, prod: product, is_quick: false, column: 'left' -%}
      {%- endcapture -%}
      {% unless left_side_blocks == blank %}
        <div class="left-side-blocks for-desktop">
          {{ left_side_blocks }}
        </div>
      {% endunless %}
    </div>

    <div class="product__details product__primary-right">
      <div class="product__meta">
        {%- render 'product-blocks'
          , blocks: section.blocks
          , prod: product
          , is_quick: false
          , column: 'right'
          , current_variant: current_variant
          , featured_media: featured_media
          -%}
        {%- comment -%}
          Live region for announcing updated price and availability to screen readers
        {%- endcomment -%}
        <p class="visually-hidden" data-product-status aria-live="polite" role="status"></p>

        {%- comment -%}
          Live region for announcing that the product form has been submitted and the
          product is in the process being added to the cart
        {%- endcomment -%}
        <p class="visually-hidden" data-loader-status aria-live="assertive" role="alert" aria-hidden="true">
          {{ 'products.product.loader_label' | t }}
        </p>

        {%- comment -%}Shell for Shopify product review accordion hidden by default{%- endcomment -%}
        {%- assign review_title = 'products.product.reviews' | t -%}
        {%- render 'product-block-accordion'
          , classes: 'product-reviews hidden'
          , heading: review_title
          , small_text: true
          -%}
      </div>
    </div>
  </div>

  <div class="product__bottom">
    {% unless left_side_blocks == blank %}
      <div class="left-side-blocks for-mobile">
        {{ left_side_blocks }}
      </div>
    {% endunless %}
  </div>
</div>

{%- assign models = product.media | where: 'media_type', 'model' | json -%}

<script>
  window.ShopifyXR = window.ShopifyXR || function() {
    (ShopifyXR.q = ShopifyXR.q || []).push(arguments)
  }
  ShopifyXR('addModels', {{ models }});
</script>
<script src="//cdn.shopify.com/shopifycloud/shopify-xr-js/assets/v1.0/shopify-xr.en.js" defer="defer"></script>

{%- unless product == empty -%}
  <script type="application/json" id="ProductJson-{{ section.id }}">
    {{ product | json }}
  </script>
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{%- endunless -%}

<script type="application/ld+json">
  {
    "@context": "http://schema.org/",
    "@type": "Product",
    "name": {{ product.title | json }},
    "url": {{ product_url | json }},{%- if product.featured_media -%}
      "image" : [{{ product_featured_image_url }}],{%- endif -%}
    "description": {{ product.description | strip_html | json }},{%- if current_variant.sku != blank -%}
      "sku" : {{ current_variant.sku | json }},{%- endif -%}
    "brand": {
      "@type": "Thing",
      "name": {{ product.vendor | json }}
    },
    "offers": [{%- for variant in product.variants -%}
        {
          "@type": "Offer",
          {%- if variant.sku != blank -%}
            "sku" : {{ variant.sku | json }},
          {%- endif -%}
          "availability": "http://schema.org/ {% if variant.available %}InStock
          {% else %}
            OutOfStock{% endif %}",
          "price": {{ variant.price | divided_by: 100.00 | json }},
          "priceCurrency": {{ cart.currency.iso_code | json }},
          "url": {{ shop.url | append: variant.url | json }}
        }
        {% unless forloop.last %},{% endunless %}{%- endfor -%}]
  }
</script>

